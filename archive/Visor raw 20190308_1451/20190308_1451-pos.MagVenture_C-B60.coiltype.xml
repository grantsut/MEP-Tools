<?xml version="1.0" encoding="UTF-8"?>
<COIL_TYPES>
<coil>
	<manufacturer>MagVenture</manufacturer> 
	<model>C-B60</model> <!-- Important: Model string must be part of configuration file name using pattern: "modelstring.coiltype.xml".-->
  <coilTools>
    <coilTool>NT-112.rom</coilTool>
	<coilTool>NT-143.rom</coilTool>
  </coilTools>
  <calibrationTools>
    <calibrationTool>NT-104.rom</calibrationTool>
  </calibrationTools>
	
	<loop>
		<discretization>2</discretization>
		<discretizationOffline>4</discretizationOffline>
		<radius>0.0375</radius>
		<windings>10</windings>
		<center>
			<x>0.0375</x>
			<y>0.0</y>
			<z>0.0</z>
		</center>
		<normal>
			<x>0.0</x>
			<y>0.0</y>
			<z>1.0</z>
		</normal>	
	</loop>
	
	<loop>
		<discretization>2</discretization>
		<discretizationOffline>4</discretizationOffline>
		<radius>0.0375</radius>
		<windings>10</windings>
		<center>
			<x>-0.0375</x>
			<y>0.0</y>
			<z>0.0</z>
		</center>
		<normal>
			<x>0.0</x>
			<y>0.0</y>
			<z>-1.0</z>
		</normal>	
	</loop>
	
	<fieldOffset>
			<x>0.0</x>
			<y>0.0</y>
			<z>0.0</z>
	</fieldOffset>			
</coil>
</COIL_TYPES>